{
    "name": "fstr-css",
    "version": "0.4.0",
    "description": "A CSS Framework",
    "main": "dist/full.css",
    "scripts": {
        "sass:dev": "node-sass scss -o demo/dist --source-map=true",
        "sass:dev:watch": "npm run sass:dev && node-sass -w scss -o demo/dist --source-map=true",
        "sass:prod": "node-sass scss -o dist --output-style=compressed",
        "dev": "live-server --open=demo"
    },
    "keywords": [
        "CSS",
        "Framework",
        "Components"
    ],
    "author": "Arjen Postma <npm@arjenpostma.nl>",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ArjenPostma/fstr.git"
    },
    "bugs": {
        "url": "https://github.com/ArjenPostma/fstr/issues"
    },
    "homepage": "https://github.com/ArjenPostma/fstr#readme",
    "devDependencies": {
        "live-server": "^1.2.1",
        "node-sass": "^7.0.0",
        "glob-parent": ">=5.1.2"
    }
}
